{
  "name": "smashmine-api",
  "version": "0.1.0",
  "description": "The node api for smashmine.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/telaviv/smashmine-api"
  },
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node",
    "test": "jest",
    "lint": "eslint src",
    "preinstall": "node -e \"var d='node_modules/src',fs=require('fs');fs.stat(d,function(e){!e&&fs.unlinkSync(d)});\"",
    "postinstall": "node -e \"var s='../src',d='node_modules/src',fs=require('fs');fs.stat(d,function(e){e&&fs.symlinkSync(s,d,'dir')});\""
  },
  "precommit": [
    "lint",
    "test"
  ],
  "keywords": [
    "smash brothers",
    "rankings",
    "glicko"
  ],
  "author": "Shawn Krisman",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^20.0.3",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2"
  }
}
